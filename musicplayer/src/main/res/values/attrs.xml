<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="trackSize" format="dimension" />
    <attr name="trackFColor" format="color" />
    <attr name="trackSColor" format="color" />
    <attr name="trackTColor" format="color" />
    <attr name="thumWidth" format="dimension" />
    <attr name="thumHeight" format="dimension" />
    <attr name="max" format="integer" />
    <attr name="min" format="integer" />

    <attr name="density" format="float" />
    <attr name="color" format="color" />
    <attr name="type" format="string" />
    <attr name="width" format="dimension" />
    <attr name="speed" format="string" />


    <!--音乐播放器的进度条-->
    <declare-styleable name="MusicSeekBar">
        <attr name="trackSize" />
        <attr name="trackFColor" />
        <attr name="trackSColor" />
        <attr name="trackTColor" />
        <attr name="thumWidth" />
        <attr name="thumHeight" />
        <attr name="max" />
        <attr name="min" />

    </declare-styleable>

    <!--音频律动-->
    <declare-styleable name="BaseVisualizer">
        <attr name="density" />
        <attr name="color" />
        <attr name="type" />
        <attr name="width" />
        <attr name="speed" />
    </declare-styleable>


</resources>